{
  "name": "webcrypto",
  "version": "0.1.1",
  "summary": "Native support for package:webcrypto",
  "description": "Wraps BoringSSL symbols required by the dart:ffiside of package:webcrypto",
  "homepage": "https://github.com/google/webcrypto.dart",
  "license": {
    "file": "../LICENSE"
  },
  "authors": {
    "Jonas Finnemann Jensen": "jonasfj@google.com"
  },
  "source": {
    "path": "."
  },
  "swift_versions": "5.0",
  "ios": {
    "dependencies": {
      "Flutter": [

      ]
    }
  },
  "osx": {
    "dependencies": {
      "FlutterMacOS": [

      ]
    }
  },
  "platforms": {
    "ios": "9.0",
    "osx": "10.11"
  },
  "public_header_files": "Classes/**/*.h",
  "source_files": [
    "Classes/**/*",
    "third_party/boringssl/**/*.{c,h}"
  ],
  "compiler_flags": [
    "-DOPENSSL_NO_ASM",
    "-DDOPENSSL_SMALL",
    "-GCC_WARN_INHIBIT_ALL_WARNINGS",
    "-w"
  ],
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": [
      "$(PODS_TARGET_SRCROOT)/../third_party/boringssl/src/include"
    ],
    "DEFINES_MODULE": "YES",
    "VALID_ARCHS[sdk=iphonesimulator*]": "x86_64"
  },
  "swift_version": "5.0"
}
